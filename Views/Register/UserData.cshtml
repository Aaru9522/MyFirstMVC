@model MyFirstMVC.Models.Register

@{
    ViewBag.Title = "UserData";
}

<h2>UserData</h2>



<p><strong>User ID:</strong> @Model.UserId</p>
<p><strong>User Name:</strong> @Model.UserName</p>
<p><strong>Email:</strong> @Model.Email</p>
<p><strong>Password:</strong> @Model.Password</p>
<p><strong>Gender:</strong> @Model.Gender</p>
<p><strong>Role:</strong> @Model.Role</p>

@if (Model.Hobbies != null && Model.Hobbies.Any())
{
    <p><strong>Hobbies:</strong></p>
    <ul>
        @foreach (var hobby in Model.Hobbies)
        {
            <li>@hobby</li>
        }
    </ul>
}
